<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="session3.LoadingPage" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Welcome">
		<content>
			<VBox>
				<HBox>
					<Label text="Enter username" required="true" design="Bold"></Label>
					<Input width="200px" type="Number" maxLength="10"></Input>
					<Button text="click me" press="onClick"></Button>
				</HBox>
				<HBox>
					<Button text="click me" type="Emphasized" press="onClick_E"></Button>
					<Button text="click me" type="Accept" press="onClick_A"></Button>
					<Button text="click me" type="Reject" press="onClick_R"></Button>
				</HBox>
			</VBox>
		</content>
	</Page>
</core:View>